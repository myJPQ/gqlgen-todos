# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type Todo {
  id:Int!
  meeting:Meeting!
}

type Meeting{
  title: String!
  description: String!
  startTime: Int!

}


type Query {
  getmeeting(id: Int!):Todo
}

input NewTodo {
id:Int!
title: String!
  description: String!
  startTime: Int!

}


type Mutation {
  updateMeeting(input: NewTodo): Todo!
}
